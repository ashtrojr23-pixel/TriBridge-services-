#tb-close{position:absolute;top:12px;right:14px;background:none;border:none;color:rgba(255,255,255,0.5);font-size:1.3rem;cursor:pointer;transition:color 0.2s;line-height:1;}
#tb-close:hover{color:#C9A84C;}
#tb-chat-area{padding:20px 16px;min-height:160px;display:flex;flex-direction:column;gap:12px;max-height:260px;overflow-y:auto;}
.tb-msg{max-width:82%;padding:10px 14px;font-size:0.9rem;line-height:1.6;border-radius:2px;opacity:0;transform:translateY(6px);animation:tbMsgIn 0.4s ease forwards;}
@keyframes tbMsgIn{to{opacity:1;transform:translateY(0);}}
.tb-msg.bot{background:linear-gradient(135deg,rgba(61,43,31,0.8),rgba(26,14,5,0.9));border:1px solid rgba(201,168,76,0.3);border-left:3px solid #C9A84C;color:#DEC89A;align-self:flex-start;}
.tb-msg.bot strong{display:block;font-family:'Cinzel',serif;color:#F0D080;font-size:0.75rem;letter-spacing:1px;margin-bottom:4px;}
.tb-msg.user{background:linear-gradient(135deg,#128C7E,#075E54);border:1px solid rgba(37,211,102,0.3);color:#fff;align-self:flex-end;text-align:right;}
#tb-quick-replies{display:flex;flex-wrap:wrap;gap:8px;padding:0 16px 16px;}
.tb-qr{padding:7px 14px;background:rgba(201,168,76,0.08);border:1px solid rgba(201,168,76,0.4);color:#F0D080;font-family:'EB Garamond',serif;font-size:0.82rem;cursor:pointer;transition:all 0.3s;border-radius:2px;}
.tb-qr:hover{background:#C9A84C;color:#1A0A00;border-color:#C9A84C;}
#tb-popup-footer{border-top:1px solid rgba(201,168,76,0.2);padding:14px 16px;}
