.tb-typing{display:flex;gap:5px;align-items:center;padding:10px 14px;background:linear-gradient(135deg,rgba(61,43,31,0.8),rgba(26,14,5,0.9));border:1px solid rgba(201,168,76,0.3);border-left:3px solid #C9A84C;align-self:flex-start;border-radius:2px;opacity:0;animation:tbMsgIn 0.4s ease forwards;}
